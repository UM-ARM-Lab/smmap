<launch>
    <param name="logging_enabled" value="true"/>
    <param name="log_folder" value="$(find smmap)/../logs/"/>

    <param name="deformable_type" value="rope"/>
    <param name="task_type" value="coverage"/>

    <include file="$(find deform_simulator)/launch/deform_simulator.launch"/>

    <node name="smmap_planner_node"
        pkg="smmap"
        type="smmap_test_node"
        output="screen"/>

    <!--node name="visualizer"
        pkg="matplotlib_wrapper"
        type="matplotlib_wrapper.py"/-->
</launch>
